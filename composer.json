{
	"name": "drago-ex/project-db",
	"type": "composer-plugin",
	"description": "The package extends the Drago Project to include database work.",
	"require": {
		"php": ">=8.3 <9",
		"drago-ex/database": "^2.0",
		"composer-plugin-api": "^2.3"
	},
	"autoload": {
		"psr-4": {
			"Drago\\\\DatabasePlugin\\\\": "src/"
		}
	},
	"extra": {
		"class": "Drago\\\\DatabasePlugin\\\\Composer\\\\Plugin"
	},
	"require-dev": {
		"composer/composer": "^2.9"
	}
}
